{
    "name": "laravel/laravel",
    "type": "project",
    "description": "The Laravel Framework.",
    "keywords": [
        "framework",
        "laravel"
    ],
    "license": "MIT",
    "require": {
        "php": "^8.0",
        "astrotomic/laravel-translatable": "^11.1",
        "bensampo/laravel-enum": "^v4.0.0",
        "benwilkins/laravel-fcm-notification": "^4.0",
        "facade/ignition": "^2.3.6",
        "fideloper/proxy": "^4.0",
        "fruitcake/laravel-cors": "^2.0",
        "guzzlehttp/guzzle": "^7.0.1",
        "intervention/image": "^2.4",
        "jenssegers/model": "^1.2",
        "jpgraph/jpgraph": "^4.0",
        "laracasts/flash": "^3.0",
        "laravel/framework": "^8.0",
        "laravel/helpers": "^1.2",
        "laravel/passport": "^10.1",
        "laravel/scout": "^8.0",
        "laravel/socialite": "^5.0",
        "laravel/tinker": "^2.0",
        "laravel/ui": "^3.0",
        "laravelcollective/html": "^6.1",
        "league/flysystem-aws-s3-v3": "^1.0",
        "mcamara/laravel-localization": "^1.3.19",
        "onethirtyone/laravel-s3-migrate": "^2.2",
        "owen-it/laravel-auditing": "^v13.0.2",
        "pdfanonymous/phpanonymous": "dev-master",
        "predis/predis": "^1.1",
        "propaganistas/laravel-phone": "^4.2",
        "spatie/fractalistic": "^2.8",
        "spatie/laravel-backup": "^6.11",
        "spatie/laravel-event-sourcing": "^5.0",
        "spatie/laravel-fractal": "^5.5",
        "spatie/laravel-queued-db-cleanup": "^1.1.2",
        "twilio/sdk": "^6.42"
    },
    "require-dev": {
        "barryvdh/laravel-debugbar": "^3.2",
        "barryvdh/laravel-ide-helper": "^2.8",
        "beyondcode/laravel-dump-server": "^1.4",
        "doctrine/dbal": "^2.10",
        "filp/whoops": "^2.0",
        "fzaninotto/faker": "^1.4",
        "mockery/mockery": "^1.0",
        "nunomaduro/collision": "^5.0",
        "phploc/phploc": "*",
        "phpmd/phpmd": "^2.8",
        "phpmetrics/phpmetrics": "^2.7",
        "phpunit/phpunit": "^9.0",
        "squizlabs/php_codesniffer": "^3.5"
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/"
        },
        "files": [
            "app/Modules/Helpers/Functions.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/",
            "Swis\\JsonApi\\Client\\": "vendor/elwafa/json-api-client/src/"
        },
        "files": [
            "tests/utilities/functions.php"
        ]
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ]
    }
}
